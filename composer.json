{
  "name": "herojhc/laravel-repository",
  "description": "Laravel Repository",
  "license": "MIT",
  "homepage": "https://www.wfhtmc.com",
  "keywords": ["laravel", "repository", "repositories", "eloquent", "database"],
  "authors": [
    {
      "name": "herojhc",
      "email": "759242210@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/http": "~5.5",
    "illuminate/config": "~5.5",
    "illuminate/support": "~5.5",
    "illuminate/database": "~5.5",
    "illuminate/filesystem": "~5.5",
    "illuminate/console": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "mockery/mockery": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Herojhc\\": "src/Herojhc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Herojhc\\Repositories\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    },
    "laravel": {
      "providers": [
        "Herojhc\\Repositories\\Providers\\RepositoryProvider"
      ]
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}
