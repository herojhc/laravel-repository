{
  "name": "herojhc/laravel-repository",
  "description": "Laravel Repository",
  "license": "MIT",
  "homepage": "https://www.wfhtmc.com",
  "keywords": ["laravel", "repository", "repositories", "eloquent", "database"],
  "authors": [
    {
      "name": "herojhc",
      "email": "759242210@qq.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "^5.3",
    "illuminate/database": "^5.3",
    "illuminate/filesystem": "^5.3",
    "illuminate/console": "^5.3"
  },
  "require-dev": {
    "mockery/mockery": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Bosnadev\\": "src/Bosnadev/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bosnadev\\Repositories\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
